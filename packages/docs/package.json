{
	"name": "@timonteutelink/code-templator-docs",
	"private": true,
	"packageManager": "bun@1.2.15",
	"scripts": {
		"dev": "docusaurus start --port 3100",
		"clean": "rm -rf build .docusaurus src/docs/lib src/docs/cli dist temp",
		"gen:api": "api-extractor run --local --verbose",
		"gen:cli": "cd ../../apps/cli && oclif readme --multi --output-dir=../../packages/docs/src/docs/cli && cd ../../packages/docs",
		"docs": "bun run gen:api && bun run gen:cli && docusaurus build",
		"devdocs": "bun run gen:api && bun run gen:cli && docusaurus start --port 3100",
		"serve": "docusaurus serve"
	},
	"devDependencies": {
		"@docusaurus/core": "^3.8.0",
		"@docusaurus/preset-classic": "^3.8.0",
		"@microsoft/api-extractor": "^7.52.8",
		"clsx": "^2.1.1",
		"commander": "^14.0.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"typedoc": "^0.28.5",
		"docusaurus-plugin-typedoc": "^1.4.0",
		"typedoc-plugin-markdown": "^4.6.4"
	}
}
