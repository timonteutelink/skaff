{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"mainEntryPointFilePath": "../skaff-lib/dist/index.d.ts",
	"projectFolder": ".",
	"apiReport": {
		"enabled": false,
		"reportFileName": "skaff-lib.api.md",
		"reportFolder": "src/docs/skaff-lib"
	},
	"docModel": {
		"enabled": true,
		"apiJsonFilePath": "temp/skaff-lib.api.json"
	},
	"dtsRollup": {
		"enabled": true,
		"publicTrimmedFilePath": "dist/skaff-lib.d.ts"
	},
	"messages": {
		"compilerMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		},
		"extractorMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		}
	}
}
