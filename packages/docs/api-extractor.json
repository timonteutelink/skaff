{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"mainEntryPointFilePath": "../code-templator-lib/dist/index.d.ts",
	"projectFolder": ".",
	"apiReport": {
		"enabled": false,
		"reportFileName": "code-templator-lib.api.md"
	},
	"docModel": {
		"enabled": true,
		"apiJsonFilePath": "temp/code-templator-lib.api.json"
	},
	"dtsRollup": {
		"enabled": true,
		"publicTrimmedFilePath": "dist/code-templator-lib.d.ts"
	},
	"messages": {
		"compilerMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		},
		"extractorMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		}
	}
}
