{
  "name": "@timonteutelink/code-templator",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "importMap": "./import_map.json",
  "nodeModulesDir": "auto",
  "tasks": {
    "prepare": "DENO_NO_PACKAGE_JSON=1 deno install -A --unstable-sloppy-imports --unstable-detect-cjs --entrypoint src/main.ts --lock ./deno.lock",
    "build": "DENO_NO_PACKAGE_JSON=1 deno compile -A --unstable-sloppy-imports --unstable-detect-cjs --lock ./deno.lock --output=./code-templator src/main.ts ",
    "start": "DENO_NO_PACKAGE_JSON=1 deno run -A --unstable-sloppy-imports --unstable-detect-cjs --lock ./deno.lock src/main.ts",
    "lock": "DENO_NO_PACKAGE_JSON=1 deno cache -r --lock=deno.lock src/main.ts",
    "format": "deno fmt"
  }
}
