{
  "name": "@timonteutelink/skaff",
  "description": "A CLI tool for managing code templates",
  "version": "0.0.6",
  "author": "timonteutelink",
  "bin": {
    "skaff": "./bin/run.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": "https://github.com/timonteutelink/skaff/issues",
  "dependencies": {
    "@types/node": "^24.10.1",
    "@inquirer/prompts": "^7.10.1",
    "@inquirer/core": "^10.3.2",
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.36",
    "@oclif/plugin-plugins": "^5.4.54",
    "@timonteutelink/skaff-lib": "0.0.97",
    "@timonteutelink/template-types-lib": "0.0.56",
    "esbuild": "^0.27.0",
    "loglevel": "^1.9.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  },
  "overrides": {
    "@inquirer/core": "^10.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@types/bun": "^1.3.2",
    "@oclif/plugin-update": "^4.7.15",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.15",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "chai": "^6.2.1",
    "eslint": "^9.39.1",
    "eslint-config-oclif": "^6.0.117",
    "eslint-config-prettier": "^10.1.8",
    "mocha": "^11.7.5",
    "oclif": "^4.22.44",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/timonteutelink/skaff",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "skaff",
    "dirname": "skaff",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "macos": {
      "identifier": "com.timonteutelink.skaff"
    },
    "topicSeparator": " ",
    "topics": {
      "code-templating": {
        "description": "Manage code templates"
      }
    },
    "repositoryPrefix": "https://github.com/timonteutelink/skaff/tree/main/apps/cli/<%- commandPath %>"
  },
  "repository": "timonteutelink/skaff",
  "scripts": {
    "rundev": "",
    "build": "rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "bun run lint",
    "prepack": "bun run build:dist && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "__buildprod": "tsc --noEmit && bun run build.ts && bun build --compile ./dist/main.js ./dist/*.js --outfile ./bin/skaff",
    "build:dist": "bun run build && oclif manifest",
    "prepack:all": "bun install --frozen-lockfile",
    "pack:all": "bun run build:dist && oclif pack tarballs --targets linux-x64,linux-arm64,darwin-x64,darwin-arm64 && oclif pack deb",
    "postpack:all": "rm -f oclif.manifest.json",
    "pack:here": "npm run build:dist && oclif pack tarballs --targets linux-x64"
  },
  "types": "dist/index.d.ts"
}
