{
  "name": "code-templator",
  "description": "A CLI tool for managing code templates",
  "version": "0.0.0",
  "author": "timonteutelink",
  "bin": {
    "code-templator": "./bin/run.js"
  },
  "bugs": "https://github.com/timonteutelink/code-templator/issues",
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@timonteutelink/code-templator-lib": "0.0.56",
    "@timonteutelink/template-types-lib": "0.0.43",
    "@types/node": "^22",
    "esbuild": "^0.25.5",
    "pino": "^9.7.0",
    "yaml": "^2.8.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/plugin-update": "^4.6.42",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^5",
    "@types/mocha": "^10",
    "bun-plugin-pino": "^1.4.0",
    "chai": "^5",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^11",
    "oclif": "^4",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/timonteutelink/code-templator",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "code-templator",
    "dirname": "code-templator",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update"
    ],
    "macos": {
      "identifier": "com.timonteutelink.code-templator"
    },
    "topicSeparator": " ",
    "topics": {
      "code-templating": {
        "description": "Manage code templates"
      }
    },
    "repositoryPrefix": "https://github.com/timonteutelink/code-templator/tree/main/apps/cli/<%- commandPath %>"
  },
  "repository": "timonteutelink/code-templator",
  "scripts": {
    "rundev": "",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "bun run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "__buildprod": "tsc --noEmit && bun run build.ts && bun build --compile ./dist/main.js ./dist/*.js --outfile ./bin/code-templator",
    "build:dist": "bun run build && oclif manifest",
    "prepack:tarballs": "npm install --package-lock-only && npm shrinkwrap --production",
    "pack:tarballs": "npm run build:dist && oclif pack tarballs --targets linux-x64,linux-arm64,darwin-x64,darwin-arm64,win-x64",
    "postpack:tarballs": "shx rm -f oclif.manifest.json npm-shrinkwrap.json",
    "pack:here": "npm run build:dist && oclif pack tarballs --targets linux-x64"
  },
  "types": "dist/index.d.ts"
}
